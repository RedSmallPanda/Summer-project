<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sjtu.jpw.Domain.Coupon" table="Coupon" schema="jpw">
        <id name="couponId">
            <column name="coupon_id" sql-type="int(11)"/>
        </id>
        <property name="title">
            <column name="title" sql-type="text" not-null="true"/>
        </property>
        <property name="discount">
            <column name="discount" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="discCond">
            <column name="disc_cond" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="begindate">
            <column name="begindate" sql-type="date" not-null="true"/>
        </property>
        <property name="enddate">
            <column name="enddate" sql-type="date" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>