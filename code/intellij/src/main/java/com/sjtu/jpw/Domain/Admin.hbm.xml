<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sjtu.jpw.Domain.Admin" table="Admin" schema="jpw">
        <id name="adminId">
            <column name="admin_id" sql-type="int(11)"/>
        </id>
        <property name="username">
            <column name="username" sql-type="text" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="text" not-null="true"/>
        </property>
        <property name="level">
            <column name="level" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>