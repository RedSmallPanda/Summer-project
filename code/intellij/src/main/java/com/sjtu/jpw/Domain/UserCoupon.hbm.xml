<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sjtu.jpw.Domain.UserCoupon" table="User_Coupon" schema="jpw">
        <composite-id mapped="true" class="com.sjtu.jpw.Domain.UserCouponPK">
            <key-property name="userId">
                <column name="user_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="couponId">
                <column name="coupon_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="number">
            <column name="number" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>