<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sjtu.jpw.Domain.Orders" table="Orders" schema="jpw">
        <id name="orderId">
            <column name="order_id" sql-type="int(11)"/>
        </id>
        <property name="number">
            <column name="number" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="state">
            <column name="state" sql-type="text" not-null="true"/>
        </property>
        <property name="time">
            <column name="time" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>